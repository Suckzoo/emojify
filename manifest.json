{
  "name": "emojify",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "emojify like slack",
  "homepage_url": "http://danharper.me",
  "background": {
    "scripts": [
      "background.js",
      "index.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "thinking.png",
    "default_title": "emojify enabled"
  },
  "permissions": [
    "https://twitter.com/*",
    "tabs"
  ]
}
